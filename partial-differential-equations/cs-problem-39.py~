

from scipy import fft, ifft
from scipy.integrate import odeint
import numpy as np
import matplotlib.pyplot as plt
from numpy import exp,arange
import math


def f (t) :
	return t*t;


t   = np.arange(0, 16, 0.25)
sin = map(f, t) #np.sin(t)
print t


plt.subplot(411)
plt.plot(t, sin)


plt.subplot(412)
fftSin = fft(sin)
freq = np.fft.fftfreq(t.shape[-1])
plt.plot(freq, fftSin.real)

plt.subplot(413)
plt.plot(freq, fftSin.real)

plt.subplot(414)
plt.plot(t, ifft(fftSin))

plt.show()
